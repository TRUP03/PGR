  <div class="form3">
    <div class="div1">

      <!-- images -->
      <div class="wrapper">

        <div class="images-area">
          <img src="/images/cdac-4.jpg" alt="image" class="firstImage">
          <img width=835.558px; style="margin-left:0px;" src="/images/cdac-5.jpg" alt="image" class="firstImage">
          <img width=835.558px; style="margin-left:0px;" src="/images/img5.jpg" alt="image"class="firstImage">

          <img width=835.558px; style="margin-left:0px;" src="/images/cdac-1.jpg" alt="image" class="firstImage">
          <img width=835.558px; style="margin-left:0px;" src="/images/cdac_programme.jpg" alt="image"
            class="firstImage">
          <img width=835.558px; style="margin-left:0px;" src="/images/cdac-2.jfif" alt="image" class="firstImage">
          <img width=835.558px; style="margin-left:0px;" src="/images/cdac5.png" alt="image" class="firstImage">


        </div>

        <div class="buttons-area">
          <div class="previous-btn">
            <i class='bx bx-chevron-left'></i>
          </div>
          <div class="next-btn">
            <i class='bx bx-chevron-right'></i>
          </div>
        </div>

        <div class="pagination-area">
        </div>
      </div>
      <!-- images end -->
    </div>
    <div class="div2">

      <% if(message.length > 0){ %>
      <div class="alert">
        <%= message %>
      </div>
      <% } %>


      <div class="tab">
        <button class="tablinks" onclick="openCity(event, 'Userlogin')" id="defaultOpen">USER LOGIN USING
          PASSWORD</button>
        <button class="tablinks" onclick="openCity(event, 'otplogin')">USER LOGIN USING OTP</button>

      </div>


      <div id="Userlogin" class="tabcontent">
        <!-- <form action="#" class="form"> -->
        <form class="form" action="/login" method="post">

          <h1 class="form_title"> Login Using Password </h1>
          <div class="form_div">

            <p>
              Username/Email Id/Mobile No
            </p>
            <input type="text" name="username" class="form_input" placeholder="Username">
          </div>

          <div class="form_div">

            <p>
              Password
            </p>
            <input type="password" name="password" class="form_input" placeholder="Password"><br>
          </div>
          <p style="font-size: 25px; color:#f79d9d;">
            Security Code
          </p>
          <div id="captcha" class="form_div">
            <input type="hidden" name="captcha" value="" class="input">
            <div class="preview"></div>
            <div class="captcha_form">
              <input type="text" name="inputCaptcha" id="captcha_form" class="form_input_captcha"
                placeholder="Security Code">
              <input type="button" class="captcha_refersh"></input>
            </div>
          </div>
          <input type="submit" class="form_button" value="LOGIN">
          <div class="link1">

            <ul>

              <li><a href="/forgotPass">Forgot password?</a></li>
              <!-- <li><a href="/forgotUsername">Forgot username?</a></li> -->
              <li><a href="/registration">Not a member? Register</a></li>
            </ul>
          </div>
        </form>
      </div>

      <div id="otplogin" class="tabcontent">
        <!-- <h4> LOGIN USING OTP</h4> -->
        <form class="form" action="/login" method="post">

          <h1 class="form_title"> Login Using Otp </h1>
          <div class="form_div">

            <p>
              Username/Email Id/Mobile No
            </p>
            <input type="text" name="username" class="form_input" placeholder="Username">
          </div>

          <div class="form_div">

            <p>
              OTP
            </p>
            <div class="otp">
              <input type="OTP" name="OTP" placeholder="Password">
              <button type="button" onclick="generate()">Generate OTP</button>
            </div>
          </div>
          <input type="submit" class="form_button" value="LOGIN">
          <div class="link2">

            <ul>

              <li><a href="/forgotPass">Forgot password?</a></li>
              <!-- <li><a href="/forgotUsername">Forgot username?</a></li> -->

              <li><a href="/registration">Not a member? Register</a></li>
            </ul>
          </div>

        </form>
      </div>
    </div>
  </div>

